{
  "name": "kmx",
  "description": "Web GUI for CNC",
  "version": "1.0.0",
  "jsnext:main": "esm/main.js",
  "main": "./index.js",
  "browser": "app/main",
  "scripts": {
    "ng": "ng",
    "lint": "tslint --fix --project ./",
    "tsc": "tsc",
    "tscw": "tsc -w",
    "serve": "webpack-dev-server --inline --progress --port 8081",
    "aot": "webpack --config config/webpack.aot.babel.js --progress --profile --bail",
    "aot:serve": "webpack-dev-server --config config/webpack.aot.babel.js --progress --profile --bail  --port 8081",
    "build": "rimraf ../kmx && webpack --config config/webpack.prod.js --progress --profile --bail",
    "build:dev": "rimraf dist && webpack --config config/webpack.dev.js --progress --profile --bail",
    "analyze": "rimraf ../kmx && webpack --config config/webpack.analyze.js --progress --profile --bail"
  },
  "license": "ISC",
